{
  "name": "carbon-budget-countdown",
  "private": true,
  "description": "A countdown for Sweden's carbon budget",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "stage": "gatsby clean && gatsby build && firebase deploy -P stage",
    "deploy": "gatsby clean && gatsby build && firebase deploy -P prod",
    "convert": "csvtojson data/csv/'${npm_config_filename}'.csv > data/'${npm_config_filename}'.json"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "gatsby": "^5.11.0",
    "gatsby-background-image": "^1.6.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-emotion": "^8.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-react-helmet": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "intl": "^1.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0",
    "victory": "^36.5.2"
  },
  "devDependencies": {
    "dotenv": "^16.0.1",
    "prettier": "^3.0.0"
  }
}
