{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "stage": "gatsby clean && gatsby build && firebase deploy -P stage",
    "deploy": "gatsby clean && gatsby build && firebase deploy -P prod",
    "convert": "csvtojson data/csv/'${npm_config_filename}'.csv > data/'${npm_config_filename}'.json" // TODO: Add cmd line argument
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.27.4",
    "gatsby-background-image": "^1.3.1",
    "gatsby-plugin-emotion": "^4.5.0",
    "gatsby-plugin-google-analytics": "^2.6.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sharp": "^2.9.1",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "intl": "^1.2.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "victory": "^35.4.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
